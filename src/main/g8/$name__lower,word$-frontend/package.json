    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "@eslint/compat": "^1.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "vite": "^6.2.6",
    "@remcovaes/web-test-runner-vite-plugin": "^1.2.2",
    "@johanblumenberg/ts-mockito": "^1.0.45",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  }
