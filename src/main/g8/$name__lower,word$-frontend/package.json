{
  "name": "$name$-frontend",
  "author": "$author_name$",
  "version": "$version$",
  "license": "Apache License, Version 2.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@tmtsoftware/esw-ts": "github:tmtsoftware/esw-ts#f80b4286d557922acdc65ba3f1aa159273e110d2",
    "antd": "4.16.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel fix",
    "test": "web-test-runner 'test/**/*.test.{ts,tsx}'",
    "fix": "npm-run-all --parallel clean fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix --config .eslintrc.json './**/*.{ts,tsx}'",
    "fix:prettier": "prettier --write --config .prettierrc './**/*.{ts,tsx}' "
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "devDependencies": {
    "@snowpack/plugin-typescript": "1.2.1",
    "@snowpack/web-test-runner-plugin": "0.2.2",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/chai": "4.2.19",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "@web/test-runner": "0.13.12",
    "chai": "4.3.4",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "snowpack": "3.6.1",
    "ts-mockito": "2.6.1",
    "typescript": "4.3.4"
  }
}
